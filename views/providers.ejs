<table>
  <thead>
    <tr>
      <% if(specialty){ %>
      <th>ID</th>
      <% } %>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Provider Specialty</th>
      <% if(specialty){ %>
      <th>Email Address</th>
      <th>Phone Number</th>
      <th>Date Joined</th>
      <% } %>
    </tr>
  </thead>

  <tbody>
    <% results.forEach(result => { %>

    <tr>
      <% if(specialty){ %>
      <td><%=result.provider_id%></td>
      <% } %>
      <td><%=result.first_name%></td>
      <td><%=result.last_name%></td>
      <td><%=result.provider_specialty%></td>
      <% if(specialty){ %>
      <td><%=result.email_address%></td>
      <td><%=result.phone_number%></td>
      <td><%=result.date_joined%></td>
      <% } %>
    </tr>

    <% }) %>
  </tbody>
</table>
